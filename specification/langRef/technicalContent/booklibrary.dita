<reference id="booklibrary" xml:lang="en-us">
    <title><xmlelement>booklibrary</xmlelement></title>
    <shortdesc>The <xmlelement>booklibrary</xmlelement> element contains the library information for
        a book.</shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm>Bookmap elements<indexterm>booklibrary</indexterm></indexterm>
                <indexterm>booklibrary</indexterm>
